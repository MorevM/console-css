{
	"name": "@morev/console.css",
	"description": "Style browser console messages with CSS",
	"type": "module",
	"version": "0.0.4",
	"private": false,
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
